                            android:layout_width="match_parent"
                            android:layout_height="@dimen/duijiangjshezhi_height"
                            android:background="@color/shezhi_hui"
                            android:orientation="vertical"
                            android:padding="5dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_marginRight="5dp">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/huise"
                                android:layout_centerVertical="true"
                                android:textSize="@dimen/info_text_normal"
                                android:text="发送\n亚音频"
                                android:gravity="center"
                                android:layout_marginBottom="5dp"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_marginTop="@dimen/margin_medium">

                                <TextView
                                    android:id="@+id/sms_ble_setting_tx_code"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:text="@string/device"
                                    android:textColor="@color/green"
                                    android:textSize="@dimen/info_text_normal"
                                    android:textStyle="bold" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ble_set_rx_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/duijiangjshezhi_height"
