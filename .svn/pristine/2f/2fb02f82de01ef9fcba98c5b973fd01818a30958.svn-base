<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content_up_data"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:id="@+id/update_title_layout"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/nitecore">
        <!--<ImageView
            android:id="@+id/update_image_return"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@mipmap/sms_return_icon1"
            android:layout_centerVertical="true"/>-->

        <TextView
            android:id="@+id/update_title_text"
            android:text="@string/sms_updata_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textSize="20dp"
            android:textColor="@color/black"/>


    </RelativeLayout>

    <com.sms.app.interphone.view.ArcProgressBar
        android:id="@+id/arcProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"/>


    <ImageView
        android:layout_width="135dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/chat_login_log_n"
        android:layout_alignParentBottom="true"
        />
</RelativeLayout>
