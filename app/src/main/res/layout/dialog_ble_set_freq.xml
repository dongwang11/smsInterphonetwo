<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/pop_root_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="30dp"
    android:paddingRight="30dp">

    <RelativeLayout
        android:id="@+id/pop_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:orientation="vertical"
            android:background="@drawable/dialog_layout_bg"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:backgroundTint="@color/nitecore">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true">

                    <TextView
                        android:id="@+id/textview1"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:id="@+id/textview2"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:layout_marginLeft="5dp"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:id="@+id/textview3"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:layout_marginLeft="5dp"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:text="."
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:id="@+id/textview4"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:id="@+id/textview5"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:layout_marginLeft="5dp"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        />
                    <TextView
                        android:id="@+id/textview6"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        android:layout_marginLeft="5dp"
                        />
                    <TextView
                        android:id="@+id/textview7"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:background="@color/bg_white_light_1"
                        android:gravity="center"
                        android:textColor="#000000"
                        android:textSize="25sp"
                        android:layout_marginLeft="5dp"
                        />




                </LinearLayout>

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.5"
                android:background="@drawable/dialog_freq_layout_bg"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/button7"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="7"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="8"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button9"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="9"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />

                 />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"

                    android:background="@color/chat_draglayout_view" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/button4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="4"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="5"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button6"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="6"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"

                    android:background="@color/chat_draglayout_view" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/button1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="1"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="2"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="3"
                        android:textSize="20sp"
                        style="@style/Widget_SKItem"
                        />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"

                    android:background="@color/chat_draglayout_view" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"

                    android:background="@drawable/dialog_freq_layout_bg">
                    <TextView
                        android:id="@+id/button_delete"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="delete"
                        android:textSize="20sp"
                        style="@style/Widget_Left_Item"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button0"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="0"
                        android:textSize="20sp"
                        style="@style/Widget_Right_Item"
                        />
                    <View
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:background="@color/chat_draglayout_view" />
                    <TextView
                        android:id="@+id/button_ok"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_weight="1"
                        android:text="ok"
                        android:textSize="20sp"
                        style="@style/Widget_Right_Item"
                        />
                </LinearLayout>


            </LinearLayout>


        </LinearLayout>


    </RelativeLayout>


</RelativeLayout>